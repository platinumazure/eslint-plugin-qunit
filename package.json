{
  "name": "eslint-plugin-qunit",
  "version": "7.2.0",
  "description": "ESLint plugin containing rules useful for QUnit tests.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test",
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint \"**/*.md\"",
    "lint:js": "eslint ./ --report-unused-disable-directives",
    "unit-test": "nyc mocha tests/**/*.js",
    "report-coverage-html": "nyc report --reporter=html --report-dir build/coverage",
    "preversion": "npm test",
    "update": "node ./scripts/update-rules.js",
    "version": "node build/generate-release-changelog.js"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "dependencies": {
    "eslint-utils": "^3.0.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.7.0",
    "all-contributors-cli": "^6.20.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.4.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "markdownlint-cli": "^0.30.0",
    "mocha": "^9.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "outdent": "^0.8.0",
    "semver": "^7.3.4",
    "shelljs": "^0.8.4",
    "typescript": "^4.5.4"
  },
  "peerDepencencies": {
    "eslint": ">=7.0.0 <9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platinumazure/eslint-plugin-qunit.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "build/**",
      "scripts/**",
      "tests/**"
    ],
    "reporter": [
      "lcovonly"
    ],
    "all": true,
    "report-dir": "./build/coverage"
  },
  "author": "Kevin Partington <kevin@kernelpanicstudios.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/platinumazure/eslint-plugin-qunit/issues"
  },
  "homepage": "https://github.com/platinumazure/eslint-plugin-qunit",
  "engines": {
    "node": "12.x || 14.x || >=16.0.0"
  }
}
